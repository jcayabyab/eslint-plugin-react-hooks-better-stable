    "bumpp": "^9.5.2",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^2.1.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcayabyab/eslint-plugin-react-hooks-better-stable.git"
  },
  "homepage": "https://github.com/jcayabyab/eslint-plugin-react-hooks-better-stable",
  "bugs": "https://github.com/jcayabyab/eslint-plugin-react-hooks-better-stable/issues"
